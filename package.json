{
  "name": "mobilitybox",
  "version": "0.0.0-development",
  "description": "A fast and easy to use wrapper for the Mobilitybox. Get scheduling data at ease.",
  "main": "dist/index.js",
  "repository": "https://github.com/themobilitybox/mobilitybox-js",
  "author": "Ren√© Meye <rene.meye@vesputi.com> (https://themobilitybox.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "5.1.1",
    "mocha": "8.2.1",
    "nock": "13.0.7",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "semantic-release": "^17.3.7",
    "webpack": "5.19.0",
    "webpack-cli": "4.4.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "commit": "cz",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename mobilitybox.js --mode development",
    "build:umd.min": "webpack --output-filename mobilitybox.min.js --mode production",
    "watch:test": "npm test -- --watch --watch-extensions",
    "test": "mocha ./src/index.test.js --require babel-core/register",
    "cover": "nyc --reporter=lcov --reporter=text-summary npm test",
    "report-coverage": "cat coverage/lcov.info | codecov",
    "check-coverage": "nyc check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "semantic-release": "semantic-release",
    "postinstall": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "axios": "0.21.1",
    "cancelable-promise": "3.2.3",
    "istanbul": "0.4.5",
    "nyc": "15.1.0"
  }
}